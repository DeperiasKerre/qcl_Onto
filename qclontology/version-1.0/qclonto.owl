    <!-- http://emmo.info/domains/emmo-material.owl#Material -->

    <owl:Class rdf:about="http://emmo.info/domains/emmo-material.owl#Material">
        <rdfs:label xml:lang="en">EMMO:Material</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/ontology/bibo/AcademicArticle -->

    <owl:Class rdf:about="http://purl.org/ontology/bibo/AcademicArticle">
        <rdfs:subClassOf rdf:resource="https://www.w3.org/TR/prov-o/#Agent"/>
        <rdfs:comment xml:lang="en">A scholarly academic article, typically published in a journal.</rdfs:comment>
        <rdfs:label xml:lang="en">Academic Article</rdfs:label>
    </owl:Class>
    


    <!-- https://ftp.ebi.ac.uk/pub/databases/chebi/ontology/#Atom -->

    <owl:Class rdf:about="https://ftp.ebi.ac.uk/pub/databases/chebi/ontology/#Atom">
        <rdfs:comment xml:lang="en">A chemical entity constituting the smallest component of an element having the chemical properties of the element.</rdfs:comment>
        <rdfs:label xml:lang="en">ChEBI:Atom</rdfs:label>
    </owl:Class>
    


    <!-- https://github.com/DeperiasKerre/qcl_Onto/blob/main/qclontology/version-1.0/qclonto.owl#HeterostructureMaterials -->

    <owl:Class rdf:about="https://github.com/DeperiasKerre/qcl_Onto/blob/main/qclontology/version-1.0/qclonto.owl#HeterostructureMaterials">
        <rdfs:comment xml:lang="en">Heterostructure Materials refers to the materials forming the heterostructure layer.</rdfs:comment>
        <rdfs:label xml:lang="en">Heterostructure Materials</rdfs:label>
    </owl:Class>
    


    <!-- https://github.com/DeperiasKerre/qcl_Onto/blob/main/qclontology/version-1.0/qclonto.owl#LaserDesignType -->

    <owl:Class rdf:about="https://github.com/DeperiasKerre/qcl_Onto/blob/main/qclontology/version-1.0/qclonto.owl#LaserDesignType">
        <rdfs:comment xml:lang="en">Represents the geometrical arrangement of the materials, for instance LO-Phonon.</rdfs:comment>
        <rdfs:label xml:lang="en">Laser Design Type</rdfs:label>
    </owl:Class>
    


    <!-- https://github.com/DeperiasKerre/qcl_Onto/blob/main/qclontology/version-1.0/qclonto.owl#LaserHeterostructure -->

    <owl:Class rdf:about="https://github.com/DeperiasKerre/qcl_Onto/blob/main/qclontology/version-1.0/qclonto.owl#LaserHeterostructure">
        <rdfs:subClassOf>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="https://github.com/DeperiasKerre/qcl_Onto/blob/main/qclontology/version-1.0/qclonto.owl#hasDesignType"/>
                        <owl:allValuesFrom rdf:resource="https://github.com/DeperiasKerre/qcl_Onto/blob/main/qclontology/version-1.0/qclonto.owl#LaserDesignType"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="https://github.com/DeperiasKerre/qcl_Onto/blob/main/qclontology/version-1.0/qclonto.owl#hasDesignType"/>
                        <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                        <owl:onClass rdf:resource="https://github.com/DeperiasKerre/qcl_Onto/blob/main/qclontology/version-1.0/qclonto.owl#LaserDesignType"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">A layered material composed of different semiconductor materials.</rdfs:comment>
        <rdfs:label xml:lang="en">Laser Heterostructure</rdfs:label>
    </owl:Class>
    


    <!-- https://github.com/DeperiasKerre/qcl_Onto/blob/main/qclontology/version-1.0/qclonto.owl#LaserWorkingMode -->

    <owl:Class rdf:about="https://github.com/DeperiasKerre/qcl_Onto/blob/main/qclontology/version-1.0/qclonto.owl#LaserWorkingMode">
        <rdfs:comment xml:lang="en">Refers to the nature in which the laser beams are emmitted.</rdfs:comment>
        <rdfs:label xml:lang="en">Laser Working Mode</rdfs:label>
    </owl:Class>
    


    <!-- https://github.com/DeperiasKerre/qcl_Onto/blob/main/qclontology/version-1.0/qclonto.owl#LayerSequence -->


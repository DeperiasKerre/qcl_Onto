    <!-- http://www.w3.org/ns/prov#wasAttributedTo -->

    <owl:ObjectProperty rdf:about="http://www.w3.org/ns/prov#wasAttributedTo">
        <rdfs:domain rdf:resource="https://github.com/DeperiasKerre/qcl_Onto/blob/main/qclontology/version-1.0/qclonto.owl#QuantumCascadeLaser"/>
        <rdfs:comment xml:lang="en">wasAttributedTo represents the relationship  quantum cascade laser device and the scientific article documenting its design and performance properties.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://github.com/DeperiasKerre/qcl_Onto/blob/main/qclontology/version-1.0/qclonto.owl#basedOnMaterials -->

    <owl:ObjectProperty rdf:about="https://github.com/DeperiasKerre/qcl_Onto/blob/main/qclontology/version-1.0/qclonto.owl#basedOnMaterials">
        <rdfs:domain rdf:resource="https://github.com/DeperiasKerre/qcl_Onto/blob/main/qclontology/version-1.0/qclonto.owl#LayerSequence"/>
        <rdfs:range rdf:resource="https://github.com/DeperiasKerre/qcl_Onto/blob/main/qclontology/version-1.0/qclonto.owl#HeterostructureMaterials"/>
        <rdfs:comment xml:lang="en">basedOnMaterials represents that the layer sequence is based on materials in the material composition of a heterostructure.</rdfs:comment>
        <rdfs:label xml:lang="en">based on materials</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://github.com/DeperiasKerre/qcl_Onto/blob/main/qclontology/version-1.0/qclonto.owl#composedOf -->

    <owl:ObjectProperty rdf:about="https://github.com/DeperiasKerre/qcl_Onto/blob/main/qclontology/version-1.0/qclonto.owl#composedOf">
        <rdfs:domain rdf:resource="https://github.com/DeperiasKerre/qcl_Onto/blob/main/qclontology/version-1.0/qclonto.owl#HeterostructureMaterials"/>
        <rdfs:range rdf:resource="https://ftp.ebi.ac.uk/pub/databases/chebi/ontology/#Atom"/>
        <rdfs:comment xml:lang="en">composedOf represents that materials are composed of atoms.</rdfs:comment>
        <rdfs:label xml:lang="en">composed of</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://github.com/DeperiasKerre/qcl_Onto/blob/main/qclontology/version-1.0/qclonto.owl#correspondsToWorkingMode -->

    <owl:ObjectProperty rdf:about="https://github.com/DeperiasKerre/qcl_Onto/blob/main/qclontology/version-1.0/qclonto.owl#correspondsToWorkingMode">

        <rdfs:range rdf:resource="https://github.com/DeperiasKerre/qcl_Onto/blob/main/qclontology/version-1.0/qclonto.owl#LayerSequence"/>
        <rdfs:comment xml:lang="en">hasLayerSequence indicates the relationship between a heterostructure and the layer sequence it has.</rdfs:comment>
        <rdfs:label xml:lang="en">has layer sequence</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://github.com/DeperiasKerre/qcl_Onto/blob/main/qclontology/version-1.0/qclonto.owl#hasMaterials -->

    <owl:ObjectProperty rdf:about="https://github.com/DeperiasKerre/qcl_Onto/blob/main/qclontology/version-1.0/qclonto.owl#hasMaterials">
        <rdfs:domain rdf:resource="https://github.com/DeperiasKerre/qcl_Onto/blob/main/qclontology/version-1.0/qclonto.owl#LaserHeterostructure"/>
        <rdfs:comment xml:lang="en">hasMaterials represents that a heterostructure has materials that form the heterostructure layer.</rdfs:comment>
        <rdfs:label xml:lang="en">has materials</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://github.com/DeperiasKerre/qcl_Onto/blob/main/qclontology/version-1.0/qclonto.owl#hasProperty -->

    <owl:ObjectProperty rdf:about="https://github.com/DeperiasKerre/qcl_Onto/blob/main/qclontology/version-1.0/qclonto.owl#hasProperty">
        <rdfs:domain rdf:resource="https://github.com/DeperiasKerre/qcl_Onto/blob/main/qclontology/version-1.0/qclonto.owl#QuantumCascadeLaser"/>
        <rdfs:range rdf:resource="https://w3id.org/mdo/core/Property"/>
        <rdfs:comment xml:lang="en">hasProperty represents the relationship between a quantum cascade laser device and the property it has.</rdfs:comment>
        <rdfs:label xml:lang="en">has property</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://github.com/DeperiasKerre/qcl_Onto/blob/main/qclontology/version-1.0/qclonto.owl#hasWorkingMode -->

    <owl:ObjectProperty rdf:about="https://github.com/DeperiasKerre/qcl_Onto/blob/main/qclontology/version-1.0/qclonto.owl#hasWorkingMode">
        <rdfs:domain rdf:resource="https://github.com/DeperiasKerre/qcl_Onto/blob/main/qclontology/version-1.0/qclonto.owl#QuantumCascadeLaser"/>
        <rdfs:range rdf:resource="https://github.com/DeperiasKerre/qcl_Onto/blob/main/qclontology/version-1.0/qclonto.owl#LaserWorkingMode"/>
        <rdfs:comment xml:lang="en">hasWorkingMode represents the relationship between a laser device and the working mode in which it is working.</rdfs:comment>
        <rdfs:label xml:lang="en">has working mode</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://github.com/DeperiasKerre/qcl_Onto/blob/main/qclontology/version-1.0/qclonto.owl#relatesToMaterial -->

    <owl:ObjectProperty rdf:about="https://github.com/DeperiasKerre/qcl_Onto/blob/main/qclontology/version-1.0/qclonto.owl#relatesToMaterial">
        <rdfs:domain rdf:resource="https://github.com/DeperiasKerre/qcl_Onto/blob/main/qclontology/version-1.0/qclonto.owl#LaserHeterostructure"/>
        <rdfs:range rdf:resource="http://emmo.info/domains/emmo-material.owl#Material"/>
        <rdfs:comment xml:lang="en">relatesToMaterial refers to the relationship between the heterostructure and the material space it comes from.</rdfs:comment>
        <rdfs:label xml:lang="en">relates to material</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://qudt.org/schema/qudt#hasQuantityValue -->

    <owl:ObjectProperty rdf:about="https://qudt.org/schema/qudt#hasQuantityValue">
        <rdfs:domain rdf:resource="https://qudt.org/schema/qudt/Quantity"/>
        <rdfs:range rdf:resource="https://qudt.org/schema/qudt/QuantityValue"/>
        <rdfs:label xml:lang="en">has quantity value</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://qudt.org/schema/qudt/hasQuantityKind -->

    <owl:ObjectProperty rdf:about="https://qudt.org/schema/qudt/hasQuantityKind">
        <rdfs:domain rdf:resource="https://qudt.org/schema/qudt/Quantity"/>
        <rdfs:range rdf:resource="https://qudt.org/schema/qudt/QuantityKind"/>
        <rdfs:label xml:lang="en">has quantity kind</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://qudt.org/schema/qudt/unit -->

    <owl:ObjectProperty rdf:about="https://qudt.org/schema/qudt/unit">
        <rdfs:domain rdf:resource="https://github.com/DeperiasKerre/qcl_Onto/blob/main/qclontology/version-1.0/qclonto.owl#LayerSequence"/>
        <rdfs:domain rdf:resource="https://qudt.org/schema/qudt/QuantityValue"/>
        <rdfs:range rdf:resource="https://qudt.org/schema/qudt/Unit"/>
        <rdfs:comment xml:lang="en">a quanity value has a unit</rdfs:comment>
        <rdfs:label xml:lang="en">unit</rdfs:label>
    </owl:ObjectProperty>
    



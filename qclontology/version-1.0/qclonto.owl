        <rdfs:range rdf:resource="https://github.com/DeperiasKerre/qcl_Onto/blob/main/qclontology/version-1.0/qclonto.owl#LaserWorkingMode"/>
        <rdfs:comment xml:lang="en">correspondsToWorkingMode represents the relationship between a property and the laser working mode under which it was obtained.</rdfs:comment>
        <rdfs:label xml:lang="en">corresponds to working mode</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://github.com/DeperiasKerre/qcl_Onto/blob/main/qclontology/version-1.0/qclonto.owl#hasDesignType -->

    <owl:ObjectProperty rdf:about="https://github.com/DeperiasKerre/qcl_Onto/blob/main/qclontology/version-1.0/qclonto.owl#hasDesignType">
        <rdfs:domain rdf:resource="https://github.com/DeperiasKerre/qcl_Onto/blob/main/qclontology/version-1.0/qclonto.owl#LaserHeterostructure"/>
        <rdfs:range rdf:resource="https://github.com/DeperiasKerre/qcl_Onto/blob/main/qclontology/version-1.0/qclonto.owl#LaserDesignType"/>
        <rdfs:comment xml:lang="en">hasDesignType indicates the relatioship between the laser heterostructure and the desgn type on whch it is based.</rdfs:comment>
        <rdfs:label xml:lang="en">has design type</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://github.com/DeperiasKerre/qcl_Onto/blob/main/qclontology/version-1.0/qclonto.owl#hasHeterostructure -->

    <owl:ObjectProperty rdf:about="https://github.com/DeperiasKerre/qcl_Onto/blob/main/qclontology/version-1.0/qclonto.owl#hasHeterostructure">
        <rdfs:domain rdf:resource="https://github.com/DeperiasKerre/qcl_Onto/blob/main/qclontology/version-1.0/qclonto.owl#QuantumCascadeLaser"/>
        <rdfs:range rdf:resource="https://github.com/DeperiasKerre/qcl_Onto/blob/main/qclontology/version-1.0/qclonto.owl#LaserHeterostructure"/>
        <rdfs:comment xml:lang="en">has heterostructure represents the relationship between a quantum cascade laser and the heterostructural design it has.</rdfs:comment>
        <rdfs:label xml:lang="en">has heterostructure</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://github.com/DeperiasKerre/qcl_Onto/blob/main/qclontology/version-1.0/qclonto.owl#hasLayerSequence -->

    <owl:ObjectProperty rdf:about="https://github.com/DeperiasKerre/qcl_Onto/blob/main/qclontology/version-1.0/qclonto.owl#hasLayerSequence">

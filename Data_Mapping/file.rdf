    rdfs:range xsd:string .

<https://github.com/DeperiasKerre/qcl_Onto/blob/main/qclontology/version-1.0/qclonto.owl#PublicationYear> a owl:DatatypeProperty ;
    rdfs:comment "PublicationYear represents that a referenceinfoAgent  has the year of publication in a an integer."@en ;
    rdfs:domain <https://github.com/DeperiasKerre/qcl_Onto/blob/main/qclontology/version-1.0/qclonto.owl#ReferenceInfoAgent> ;
    rdfs:range xsd:integer .

<https://github.com/DeperiasKerre/qcl_Onto/blob/main/qclontology/version-1.0/qclonto.owl#PublisherName> a owl:DatatypeProperty ;

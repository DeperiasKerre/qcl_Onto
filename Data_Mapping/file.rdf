    rdfs:domain <https://github.com/DeperiasKerre/qcl_Onto/blob/main/qclontology/version-1.0/qclonto.owl#ReferenceInfoAgent> ;
    rdfs:range xsd:string .

<https://github.com/DeperiasKerre/qcl_Onto/blob/main/qclontology/version-1.0/qclonto.owl#PublicationTitle> a owl:DatatypeProperty ;

    rdfs:domain <https://github.com/DeperiasKerre/qcl_Onto/blob/main/qclontology/version-1.0/qclonto.owl#Materials> ;
    rdfs:range xsd:string .

<https://qudt.org/schema/qudt/unit> a owl:ObjectProperty ;
    rdfs:label "unit"@en ;
    rdfs:comment "a quanity value has a unit"@en ;
    rdfs:domain <https://github.com/DeperiasKerre/qcl_Onto/blob/main/qclontology/version-1.0/qclonto.owl#LayerSequence>,
        <https://qudt.org/schema/qudt/QuantityValue> ;
    rdfs:range <https://qudt.org/schema/qudt/Unit> .

<https://github.com/DeperiasKerre/qcl_Onto/blob/main/qclontology/version-1.0/qclonto.owl#DesignName> a owl:DatatypeProperty ;
    rdfs:comment "DesignName represents that a Design type  has the name in a string."@en ;
    rdfs:domain <https://github.com/DeperiasKerre/qcl_Onto/blob/main/qclontology/version-1.0/qclonto.owl#LaserDesignType> ;
    rdfs:range xsd:string .

<https://github.com/DeperiasKerre/qcl_Onto/blob/main/qclontology/version-1.0/qclonto.owl#ReferenceInfoAgent> a owl:Class ;
    rdfs:label "Reference Info Agent"@en ;
    rdfs:comment "A scientific article documenting the laser design and perfomance properties."@en ;
    rdfs:subClassOf <https://www.w3.org/TR/prov-o/#Agent> .

<https://github.com/DeperiasKerre/qcl_Onto/blob/main/qclontology/version-1.0/qclonto.owl#DOI> a owl:DatatypeProperty ;
    rdfs:comment "DOI represents that a referenceinfoAgent  has the DOI in a string."@en ;
    rdfs:domain <https://github.com/DeperiasKerre/qcl_Onto/blob/main/qclontology/version-1.0/qclonto.owl#ReferenceInfoAgent> ;
    rdfs:range xsd:string .

<https://github.com/DeperiasKerre/qcl_Onto/blob/main/qclontology/version-1.0/qclonto.owl#URL> a owl:DatatypeProperty ;
    rdfs:comment "URL represents that a referenceinfoAgent  has the URL  in a string."@en ;
    rdfs:domain <https://github.com/DeperiasKerre/qcl_Onto/blob/main/qclontology/version-1.0/qclonto.owl#ReferenceInfoAgent> ;
    rdfs:range xsd:string .

<https://github.com/DeperiasKerre/qcl_Onto/blob/main/qclontology/version-1.0/qclonto.owl#sequence> a owl:DatatypeProperty ;
    rdfs:comment "sequence represents that a layer sequence has a sequence in a string."@en ;
    rdfs:domain <https://github.com/DeperiasKerre/qcl_Onto/blob/main/qclontology/version-1.0/qclonto.owl#LayerSequence> ;
    rdfs:range xsd:string .

<https://qudt.org/schema/qudt/numericValue> a owl:DatatypeProperty ;

emmo:Material a owl:Class ;
    rdfs:label "EMMO:Material"@en .

<https://ftp.ebi.ac.uk/pub/databases/chebi/ontology/#Atom> a owl:Class ;
    rdfs:label "ChEBI:Atom"@en ;
    rdfs:comment "A chemical entity constituting the smallest component of an element having the chemical properties of the element."@en .

<https://qudt.org/schema/qudt/QuantityKind> a owl:Class ;
    rdfs:label "Quantity Kind"@en ;
    rdfs:comment "A Quantity Kind is any observable property that can be measured and quantified numerically. Familiar examples include physical properties such as length, mass, time, force, energy, power, electric charge, etc."@en .

<https://qudt.org/schema/qudt/Unit> a owl:Class ;
    rdfs:label "Unit"@en ;
    rdfs:comment "A unit of measure, or unit, is a particular quantity value that has been chosen as a scale for measuring other quantities the same kind (more generally of equivalent dimension)."@en .

<https://www.w3.org/TR/prov-o/#Agent> a owl:Class ;
    rdfs:comment "An agent is something that bears some form of responsibility for an activity taking place, for the existence of an entity, or for another agent's activity."@en .

<https://github.com/DeperiasKerre/qcl_Onto/blob/main/qclontology/version-1.0/qclonto.owl#MaterialComposition> a owl:Class ;
    rdfs:label "Material Composition"@en ;
    rdfs:comment "The materials composing the stack of the heterostructure."@en .

<https://github.com/DeperiasKerre/qcl_Onto/blob/main/qclontology/version-1.0/qclonto.owl#basedOnMaterials> a owl:ObjectProperty ;
    rdfs:label "based on materials"@en ;
    rdfs:comment "basedOnMaterials represents that the layer sequence is based on materials in the material composition of a heterostructure."@en ;
    rdfs:domain <https://github.com/DeperiasKerre/qcl_Onto/blob/main/qclontology/version-1.0/qclonto.owl#LayerSequence> ;
    rdfs:range <https://github.com/DeperiasKerre/qcl_Onto/blob/main/qclontology/version-1.0/qclonto.owl#Materials> .

<https://github.com/DeperiasKerre/qcl_Onto/blob/main/qclontology/version-1.0/qclonto.owl#correspondsToWorkingMode> a owl:ObjectProperty ;
    rdfs:label "corresponds to working mode"@en ;
    rdfs:comment "correspondsToWorkingMode represents the relationship between a property and the laser working mode under which it was obtained."@en ;
    rdfs:domain mdo-core:Property ;
    rdfs:range <https://github.com/DeperiasKerre/qcl_Onto/blob/main/qclontology/version-1.0/qclonto.owl#LaserWorkingMode> .

<https://github.com/DeperiasKerre/qcl_Onto/blob/main/qclontology/version-1.0/qclonto.owl#hasDesignType> a owl:ObjectProperty ;
    rdfs:label "has design type"@en ;
    rdfs:comment "hasDesignType indicates the relatioship between the laser heterostructure and the desgn type on whch it is based."@en ;
    rdfs:domain <https://github.com/DeperiasKerre/qcl_Onto/blob/main/qclontology/version-1.0/qclonto.owl#LaserHeterostructure> ;
    rdfs:range <https://github.com/DeperiasKerre/qcl_Onto/blob/main/qclontology/version-1.0/qclonto.owl#LaserDesignType> .

<https://github.com/DeperiasKerre/qcl_Onto/blob/main/qclontology/version-1.0/qclonto.owl#WorkingModeName> a owl:DatatypeProperty ;
    rdfs:comment "WorkingModeName represents that a Laser working mode  has the name in a string."@en ;
    rdfs:domain <https://github.com/DeperiasKerre/qcl_Onto/blob/main/qclontology/version-1.0/qclonto.owl#LaserWorkingMode> ;
    rdfs:range xsd:string .

<https://qudt.org/schema/qudt/Quantity> a owl:Class ;
    rdfs:comment "A quantity is the measurement of an observable property of a particular object, event, or physical system."@en .

<https://qudt.org/schema/qudt/QuantityValue> a owl:Class ;
    rdfs:label "Quantity Value"@en .

mdo-core:Property a owl:Class ;
    rdfs:label "Property"@en ;
    rdfs:comment "A property in materials design is data pertaining to at least one material or materials system, representing a quantifiable aspect of one material or materials system."@en ;
    rdfs:subClassOf [ a owl:Class ;
            owl:intersectionOf ( [ a owl:Restriction ;
                        owl:allValuesFrom <https://github.com/DeperiasKerre/qcl_Onto/blob/main/qclontology/version-1.0/qclonto.owl#LaserWorkingMode> ;
                        owl:onProperty <https://github.com/DeperiasKerre/qcl_Onto/blob/main/qclontology/version-1.0/qclonto.owl#correspondsToWorkingMode> ] [ a owl:Restriction ;
                        owl:onClass <https://github.com/DeperiasKerre/qcl_Onto/blob/main/qclontology/version-1.0/qclonto.owl#LaserWorkingMode> ;
                        owl:onProperty <https://github.com/DeperiasKerre/qcl_Onto/blob/main/qclontology/version-1.0/qclonto.owl#correspondsToWorkingMode> ;
                        owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ] ) ],
        <https://qudt.org/schema/qudt/Quantity> .

<https://github.com/DeperiasKerre/qcl_Onto/blob/main/qclontology/version-1.0/qclonto.owl#LaserDesignType> a owl:Class ;
    rdfs:label "Laser Design Type"@en ;
    rdfs:comment "Represents the geometrical arrangement of the materials, for instance LO-Phonon."@en .

<https://github.com/DeperiasKerre/qcl_Onto/blob/main/qclontology/version-1.0/qclonto.owl#LayerSequence> a owl:Class ;
    rdfs:label "Layer Sequence"@en ;
    rdfs:comment "Reprsents the sequence and thickness of materials composing the heterostructure."@en ;
    rdfs:subClassOf [ a owl:Class ;
            owl:intersectionOf ( [ a owl:Restriction ;
                        owl:allValuesFrom <https://github.com/DeperiasKerre/qcl_Onto/blob/main/qclontology/version-1.0/qclonto.owl#Materials> ;
                        owl:onProperty <https://github.com/DeperiasKerre/qcl_Onto/blob/main/qclontology/version-1.0/qclonto.owl#basedOnMaterials> ] [ a owl:Restriction ;
                        owl:onClass <https://github.com/DeperiasKerre/qcl_Onto/blob/main/qclontology/version-1.0/qclonto.owl#Materials> ;
                        owl:onProperty <https://github.com/DeperiasKerre/qcl_Onto/blob/main/qclontology/version-1.0/qclonto.owl#basedOnMaterials> ;
                        owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ] ) ] .

<https://github.com/DeperiasKerre/qcl_Onto/blob/main/qclontology/version-1.0/qclonto.owl#QuantumCascadeLaser> a owl:Class ;
    rdfs:label "Quantum Cascade Laser"@en ;
    rdfs:comment "Semiconductor lasers that operate in the mid to far-infrared region."@en .

mdo-core:PropertyName a owl:DatatypeProperty ;
    rdfs:comment "PropertyName represents that a property has the name in a string."@en ;
    rdfs:domain mdo-core:Property ;
    rdfs:range xsd:string .

<https://github.com/DeperiasKerre/qcl_Onto/blob/main/qclontology/version-1.0/qclonto.owl#LaserHeterostructure> a owl:Class ;
    rdfs:label "Laser Heterostructure"@en ;
    rdfs:comment "A layered material composed of different semiconductor materials."@en ;
    rdfs:subClassOf [ a owl:Class ;
            owl:intersectionOf ( [ a owl:Restriction ;
                        owl:allValuesFrom <https://github.com/DeperiasKerre/qcl_Onto/blob/main/qclontology/version-1.0/qclonto.owl#LaserDesignType> ;
                        owl:onProperty <https://github.com/DeperiasKerre/qcl_Onto/blob/main/qclontology/version-1.0/qclonto.owl#hasDesignType> ] [ a owl:Restriction ;
                        owl:onClass <https://github.com/DeperiasKerre/qcl_Onto/blob/main/qclontology/version-1.0/qclonto.owl#LaserDesignType> ;
                        owl:onProperty <https://github.com/DeperiasKerre/qcl_Onto/blob/main/qclontology/version-1.0/qclonto.owl#hasDesignType> ;
                        owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ] ) ] .

<https://github.com/DeperiasKerre/qcl_Onto/blob/main/qclontology/version-1.0/qclonto.owl#LaserWorkingMode> a owl:Class ;
    rdfs:label "Laser Working Mode"@en ;
    rdfs:comment "Refers to the nature in which the laser beams are emmitted i.e either continuos or in pulse mode."@en .

<https://github.com/DeperiasKerre/qcl_Onto/blob/main/qclontology/version-1.0/qclonto.owl#Materials> a owl:Class ;
    rdfs:label "Materials"@en ;
    rdfs:comment "Materials refers to the materials contained in the laser heterostructure design"@en .

<https://github.com/DeperiasKerre/qcl_Onto/blob/main/qclontology/version-1.0/qclonto.owl#matFormula> a owl:DatatypeProperty ;
    rdfs:label "material formula"@en ;

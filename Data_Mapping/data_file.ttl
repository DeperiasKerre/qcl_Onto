    ns1:matFormula "In0.53Ga0.47As/GaAs0.51Sb0.49" .

ns1:correspondsToWorkingMode a owl:ObjectProperty ;
    rdfs:label "corresponds to working mode"@en ;
    rdfs:comment "correspondsToWorkingMode represents the relationship between a property and the laser working mode under which it was obtained."@en ;
    rdfs:domain mdo-core:Property ;
    rdfs:range ns1:LaserWorkingMode .

ns1:hasDesignType a owl:ObjectProperty ;
    rdfs:label "has design type"@en ;
    rdfs:comment "hasDesignType indicates the relatioship between the laser heterostructure and the desgn type on whch it is based."@en ;
    rdfs:domain ns1:LaserHeterostructure ;
    rdfs:range ns1:LaserDesignType .

ns1:BoundToContinum a owl:NamedIndividual,
        ns1:LaserDesignType ;
    rdfs:label "bound to continum"@en ;
    rdfs:comment "A laser design where the sequence of material in the active region is made with more or less increasing width of layers in the multistack of material."@en .

ns1:DoubleResonantPhonon a owl:NamedIndividual,
        ns1:LaserDesignType ;
    rdfs:label "double resonant phonon"@en ;
    rdfs:comment "A laser design where two quantum well are resonant."@en .

ns2:Quantity a owl:Class ;
    rdfs:comment "A quantity is the measurement of an observable property of a particular object, event, or physical system."@en .

ns1:ContinousWaveOperation a owl:NamedIndividual,
        ns1:LaserWorkingMode ;
    rdfs:label "continous wave operation"@en ;
    rdfs:comment "Continous Wave Operation refers to the laser operation mode in which laser beams are emitted continously."@en .

ns1:PulsedOperation a owl:NamedIndividual,
        ns1:LaserWorkingMode ;
    rdfs:label "pulsed operation"@en ;
    rdfs:comment "Pulse operation  refers to the laser operation mode in which laser beams are emitted in pulses."@en .

ns2:QuantityKind a owl:Class ;
    rdfs:label "Quantity Kind"@en ;
    rdfs:comment "A Quantity Kind is any observable property that can be measured and quantified numerically. Familiar examples include physical properties such as length, mass, time, force, energy, power, electric charge, etc."@en .

ns2:Unit a owl:Class ;
    rdfs:label "Unit"@en ;
    rdfs:comment "A unit of measure, or unit, is a particular quantity value that has been chosen as a scale for measuring other quantities the same kind (more generally of equivalent dimension)."@en .

mdo-core:Property a owl:Class ;
    rdfs:label "Property"@en ;
    rdfs:comment "A property in materials design is data pertaining to at least one material or materials system, representing a quantifiable aspect of one material or materials system."@en ;
    rdfs:subClassOf [ a owl:Class ;
            owl:intersectionOf ( [ a owl:Restriction ;
                        owl:allValuesFrom ns1:LaserWorkingMode ;
                        owl:onProperty ns1:correspondsToWorkingMode ] [ a owl:Restriction ;
                        owl:onClass ns1:LaserWorkingMode ;
                        owl:onProperty ns1:correspondsToWorkingMode ;
                        owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ] ) ],
        ns2:Quantity .

ns1:LaserDesignType a owl:Class ;
    rdfs:label "Laser Design Type"@en ;
    rdfs:comment "Represents the geometrical aspects of the laser design."@en .

ns1:LaserWorkingMode a owl:Class ;
    rdfs:label "Laser Working Mode"@en ;
    rdfs:comment "Refers to the nature in which the laser beams are emmitted."@en .

ns2:QuantityValue a owl:Class ;
    rdfs:label "Quantity Value"@en .

<https://qudt.org/vocab/unit/NanoM> a ns2:Unit .

ns1:LOPhononDepopulation a owl:NamedIndividual,
        ns1:LaserDesignType ;
    rdfs:label "LO Phonon Depopulation"@en ;
    rdfs:comment "LO Phonon depopulation refers to the laser design where the sequence of material in the active region is designed to provide laser effect from longitudinal Optical Phonons."@en .

ns1:LasingFrequency a owl:NamedIndividual,
        mdo-core:Property ;
    rdfs:label "lasing frequency"@en ;
    rdfs:comment "LasingFrequency refers to the frequency at which the quantum cascade lasers emits the laser beams."@en ;
    ns4:hasQuantityValue ns1:FrequencyValue ;
    ns2:hasQuantityKind <https://qudt.org/vocab/quantitykind/Frequency> .

ns1:OpticalPower a owl:NamedIndividual,
        mdo-core:Property ;
    rdfs:label "optical power"@en ;
    rdfs:comment "OpticalPower refers to the laser output power."@en ;
    ns4:hasQuantityValue ns1:PowerValue ;
    ns2:hasQuantityKind <https://qudt.org/vocab/quantitykind/Power> .

ns1:WorkingTemperature a owl:NamedIndividual,
        mdo-core:Property ;
    rdfs:label "working temperature"@en ;
    rdfs:comment "WorkingTemperature refers to the operation temperature under which the quantum cascade laser emmits the laser beams."@en ;
    ns1:correspondsToWorkingMode ns1:ContinousWaveOperation,
        ns1:PulsedOperation ;
    ns4:hasQuantityValue ns1:WorkingTempValue ;
    ns2:hasQuantityKind <https://qudt.org/vocab/quantitykind/Temperature> .

ns1:HeterostructureMaterials a owl:Class ;
    rdfs:label "Heterostructure Materials"@en ;
    rdfs:comment "Heterostructure Materials refers to the materials forming the heterostructure layer."@en .

ns1:LayerSequence a owl:Class ;
    rdfs:label "Layer Sequence"@en ;
    rdfs:comment "Reprsents the sequence and thickness of materials composing the heterostructure."@en .

ns1:QuantumCascadeLaser a owl:Class ;
    rdfs:label "Quantum Cascade Laser"@en ;
    rdfs:comment "Semiconductor lasers that operate in the mid to far-infrared region."@en .

ns1:LaserHeterostructure a owl:Class ;
    rdfs:label "Laser Heterostructure"@en ;
    rdfs:comment "A layered material composed of different semiconductor materials."@en ;
    rdfs:subClassOf [ a owl:Class ;
            owl:intersectionOf ( [ a owl:Restriction ;
                        owl:allValuesFrom ns1:LaserDesignType ;
                        owl:onProperty ns1:hasDesignType ] [ a owl:Restriction ;
                        owl:onClass ns1:LaserDesignType ;
                        owl:onProperty ns1:hasDesignType ;
                        owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ] ) ] .

<http://purl.org/ontology/bibo/AcademicArticle> a owl:Class ;

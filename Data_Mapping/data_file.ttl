    ns1:matFormula "GaAs/Al0.15Ga0.85As" .

ns1:M7 a ns1:HeterostructureMaterials ;
    ns1:matFormula "GaAs/Al0.15Ga0.85As" .

ns1:M8 a ns1:HeterostructureMaterials ;
    ns1:matFormula "GaAs/Al0.15Ga0.85As" .


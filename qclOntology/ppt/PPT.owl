<?xml version="1.0"?>
<rdf:RDF xmlns="https://github.com/DeperiasKerre/qcl_Onto"
     xml:base="https://github.com/DeperiasKerre/qcl_Onto"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:emmo="http://emmo.info/domains/emmo-material.owl#"
     xmlns:qudt="http://qudt.org/schema/qudt/"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:vann="http://purl.org/vocab/vann/"
     xmlns:dcterms="http://purl.org/dc/terms/"
     xmlns:qudt-unit="http://qudt.org/vocab/unit/">

     <owl:Ontology rdf:about="https://github.com/DeperiasKerre/qcl_Onto">
        <owl:versionIRI rdf:resource="https://github.com/DeperiasKerre/qcl_Onto"/>
        <dcterms:contributor rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Anne Laurent</dcterms:contributor>
        <dcterms:contributor rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Kenneth Maussang</dcterms:contributor>
        <dcterms:contributor rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Dickson Owuor</dcterms:contributor>
        <dcterms:created>2023-09-21</dcterms:created>
        <dcterms:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Deperias Kerre</dcterms:creator>
        <dcterms:license>https://github.com/DeperiasKerre/qcl_Onto/blob/main/LICENSE/</dcterms:license>
        <dcterms:title xml:lang="en">An Ontological Representation of QCL properties-PPT Module</dcterms:title>
        <vann:preferredNamespacePrefix>ppt</vann:preferredNamespacePrefix>
        <vann:preferredNamespaceUri>https://github.com/DeperiasKerre/qcl_Onto/ppt/</vann:preferredNamespaceUri>
        <rdfs:comment xml:lang="en">The PPT module in QCL Properties Ontological Representation</rdfs:comment>
        <rdfs:seeAlso>https://github.com/DeperiasKerre/qcl_Onto/</rdfs:seeAlso>
        <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#string">1.0</owl:versionInfo>
    </owl:Ontology>

    <!--Annotation Properties-->


    <!-- http://purl.org/dc/terms/contributor -->

    <owl:AnnotationProperty rdf:about="http://purl.org/dc/terms/contributor"/>
    


    <!-- http://purl.org/dc/terms/created -->

    <owl:AnnotationProperty rdf:about="http://purl.org/dc/terms/created"/>
    


    <!-- http://purl.org/dc/terms/creator -->

    <owl:AnnotationProperty rdf:about="http://purl.org/dc/terms/creator"/>
    


    <!-- http://purl.org/dc/terms/license -->

    <owl:AnnotationProperty rdf:about="http://purl.org/dc/terms/license"/>
    


    <!-- http://purl.org/dc/terms/title -->

    <owl:AnnotationProperty rdf:about="http://purl.org/dc/terms/title"/>
    


    <!-- http://purl.org/vocab/vann/preferredNamespacePrefix -->

    <owl:AnnotationProperty rdf:about="http://purl.org/vocab/vann/preferredNamespacePrefix"/>
    


    <!-- http://purl.org/vocab/vann/preferredNamespaceUri -->

    <owl:AnnotationProperty rdf:about="http://purl.org/vocab/vann/preferredNamespaceUri"/>


    <!-- Data types -->

     
    <!-- http://www.w3.org/2001/XMLSchema#date -->

    <rdfs:Datatype rdf:about="http://www.w3.org/2001/XMLSchema#date"/>
  
    <!-- Object Proprties -->
     
    <!-- hasQuantityKind -->

    <owl:ObjectProperty rdf:about="http://qudt.org/schema/qudt/hasQuantityKind">
        <rdfs:range rdf:resource="http://qudt.org/schema/qudt/QuantityKind"/>
    </owl:ObjectProperty>
    

    <!-- quantityValue -->

    <owl:ObjectProperty rdf:about="http://qudt.org/schema/qudt/quantityValue">
        <rdfs:range rdf:resource="http://qudt.org/schema/qudt/QuantityValue"/>
    </owl:ObjectProperty>

    <!--hasQuantityValue-->

    <owl:ObjectProperty rdf:about="https://w3id.org/mdo/core/hasQuantityValue">
        <rdfs:comment xml:lang="en">hasQuantityValue represents the relationship between a property and quantity value it takes.</rdfs:comment>
        <rdfs:label xml:lang="en">has quantity value</rdfs:label>
    </owl:ObjectProperty>
    
    <!--relatesToMaterial-->
    
     <owl:ObjectProperty rdf:about="https://github.com/DeperiasKerre/qcl_Onto/ppt/relatesToMaterial">
        <rdfs:domain rdf:resource="https://github.com/DeperiasKerre/qcl_Onto/ppt/Heterostructure"/>
        <rdfs:range rdf:resource="http://emmo.info/domains/emmo-material.owl#EMMO_4207e895_8b83_4318_996a_72cfb32acd94"/>
        <rdfs:comment xml:lang="en">relatesToMaterial represents the relationship between a heterostructure and the materials it comes from.</rdfs:comment>
        <rdfs:label xml:lang="en">relates to material</rdfs:label>
    </owl:ObjectProperty>

    <!--correspondsToHeterostructure-->
    
    <owl:ObjectProperty rdf:about="https://github.com/DeperiasKerre/qcl_Onto/ppt/correspondsToHeterostructure">
        <rdfs:domain rdf:resource="https://github.com/DeperiasKerre/qcl_Onto/ppt/Property"/>
        <rdfs:range rdf:resource="https://github.com/DeperiasKerre/qcl_Onto/ppt/Heterostructure"/>
        <rdfs:comment xml:lang="en">correspondsToHeterostructure represents the relationship between a property and the heterostructure it corresponds to.</rdfs:comment>
        <rdfs:label xml:lang="en">relates to structure</rdfs:label>
    </owl:ObjectProperty>

    <!--Data Properties-->

    <!--numericalValue-->
    
    <owl:DatatypeProperty rdf:about="http://qudt.org/schema/qudt/numericalValue">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#double"/>
    </owl:DatatypeProperty>

    <!--PropertyName-->

    <owl:DatatypeProperty rdf:about="https://w3id.org/mdo/core/PropertyName">
        <rdfs:domain rdf:resource="https://w3id.org/mdo/core/Property"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="en">PropertyName represents that a property has the name in a string.</rdfs:comment>
        <rdfs:label xml:lang="en">property name</rdfs:label>
    </owl:DatatypeProperty>

    <!--Module Clasess-->

    <!--Material Class-->

    <owl:Class rdf:about="http://emmo.info/domains/emmo-material.owl#EMMO_4207e895_8b83_4318_996a_72cfb32acd94">
        <rdfs:label xml:lang="en">Material</rdfs:label>
    </owl:Class>

    <!--Quantity Class-->

    <owl:Class rdf:about="http://qudt.org/schema/qudt/Quantity"/>
 
    
    <!--QuantityKind Class-->

    <owl:Class rdf:about="http://qudt.org/schema/qudt/QuantityKind"/>
    

    <!--QuantityValue Class-->

    <owl:Class rdf:about="http://qudt.org/schema/qudt/QuantityValue"/>
    

    <!--Unit Class-->

    <owl:Class rdf:about="http://qudt.org/schema/qudt/Unit"/>
    
    <!--Physical Property Class-->

    <owl:Class rdf:about="https://w3id.org/mdo/core/PhysicalProperty">
        <rdfs:subClassOf rdf:resource="https://w3id.org/mdo/core/Property"/>
        <rdfs:comment xml:lang="en">A physical property is an objective property over a physical system. (ie, a property pertains to the physical reality)</rdfs:comment>
        <rdfs:label xml:lang="en">Physical Property</rdfs:label>
    </owl:Class>

    <!--Property Class -->

    <owl:Class rdf:about="https://w3id.org/mdo/core/Property">
        <rdfs:subClassOf rdf:resource="http://qudt.org/schema/qudt/Quantity"/>
        <rdfs:comment xml:lang="en">A property in materials design is data pertaining to at least one material or materials system.</rdfs:comment>
        <rdfs:label xml:lang="en">Property</rdfs:label>
    </owl:Class>

    <!--Heterostructure Class -->
  
    <owl:Class rdf:about="https://github.com/DeperiasKerre/qcl_Onto/tree/main/qclOntology/ppt/">
    <owl:onProperty rdf:resource="https://github.com/DeperiasKerre/qcl_Onto/tree/main/qclOntology/ppt/relatesToMaterial"/>
                        <owl:someValuesFrom rdf:resource="https://github.com/DeperiasKerre/qcl_Onto/tree/main/qclOntology/ppt/Material"/>
    
            <rdfs:comment xml:lang="en">A heterostructure is a layered material composed of different semiconductor materials.</rdfs:comment>
        <rdfs:label xml:lang="en">Heterostructure</rdfs:label>
    </owl:Class>
 </rdf:RDF>   